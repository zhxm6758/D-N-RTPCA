    "import cv2\n",
    "import numpy as np\n",
    "import tensorly as tl\n",
    "from tensorly.decomposition import parafac\n",
    "import scipy.io as sio\n",
    "from sklearn import decomposition\n",
    "\n",
    "trainplace = \"E:\\\\YanJiuSheng\\\\6_XiaoLunWen2-pre\\\\0_shujuji\\\\AR\\\\scarf\\\\train\\\\%d.bmp\"\n",
    "saveplace=\"E:\\\\YanJiuSheng\\\\8_test\\\\0final\\\\test3\\\\AR-scarf\\\\DNTPCA\\\\\"\n",
    "number = 1000\n",
    "\n",
    "height = 165\n",
    "width = 120\n",
    "channel = 3\n",
    "\n",
    "k0 = 165\n",
    "k1 = 60\n",
    "k2 = 3\n",
    "Tmax = 200\n",
    "\n",
    "junz = np.zeros([height, width, channel], np.float32)\n",
    "img = np.zeros([height, width, channel, number], np.float32)\n",
    "nx0 = np.zeros([height, width*channel, number], np.float32)\n",
    "nx1 = np.zeros([width, height*channel, number], np.float32)\n",
    "nx2 = np.zeros([channel, height*width, number], np.float32)\n",
    "\n",
    "V0 = np.ones([height, height],np.float32)\n",
    "V1 = np.ones([width, width],np.float32)\n",
    "V2 = np.ones([channel, channel],np.float32)\n",
    "WQ0 = np.zeros([height, height, number], np.float32)\n",
    "WQ1 = np.zeros([width, width, number], np.float32)\n",
    "WQ2 = np.zeros([channel, channel, number], np.float32)\n",
    "for i in range(0, number):\n",
    "    WQ0[:, :, i] = np.eye(height)\n",
    "    WQ1[:, :, i] = np.eye(width)\n",
    "    WQ2[:, :, i] = np.eye(channel)\n",
    "\n",
    "for n in range(0, number):\n",
    "    img[:, :, :, n] = cv2.imread(trainplace%n, 1)\n",
    "    junz = junz + img[:, :, :, n]\n",
    "junz = junz/number\n",
    "sio.savemat(saveplace+\"junzhi.mat\", {'junzhi': junz})\n",
    "\n",
    "for n in range(0, number):\n",
    "    img[:, :, :, n] = img[:, :, :, n] - junz\n",
    "    \n",
    "for n in range(0, number):\n",
    "    nx0[:,:,n] = tl.unfold(img[:,:,:,n],mode=0)\n",
    "    nx1[:,:,n] = tl.unfold(img[:,:,:,n],mode=1)\n",
    "    nx2[:,:,n] = tl.unfold(img[:,:,:,n],mode=2)\n",
    "\n",
    "Obj_Rec=np.zeros([1,Tmax],np.float32)\n",
    "\n",
    "for K in range(6,k1+1,6):\n",
    "    v1=np.ones([width, K],np.float32)\n",
    "    \n",
    "    Obj_value = 0\n",
    "    \n",
    "    for mode_value in range(0,3):\n",
    "        if mode_value==0:\n",
    "            H0 = np.zeros([height, k0], np.float32)\n",
    "            for i in range(0,number):\n",
    "                yangy01 = tl.tenalg.mode_dot(img[:,:,:,i],V0.T,mode=0)\n",
    "                yangy02 = tl.tenalg.mode_dot(yangy01,v1.T,mode=1)\n",
    "                yangy03 = tl.tenalg.mode_dot(yangy02,V2.T,mode=2)\n",
    "                yangunfold00 = tl.unfold(yangy03,mode=0)\n",
    "                U, S, V = np.linalg.svd(yangunfold00.dot(yangunfold00.T))\n",
    "                WQ0 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                uq01 = np.matmul(WQ0,nx0[:,:,i])\n",
    "                uq02 = np.matmul(uq01,nx0[:,:,i].T)\n",
    "                uq0 = np.matmul(uq02,WQ0.T)\n",
    "                H0 = H0 + uq0\n",
    "            U0, Q0, W0 = decomposition.randomized_svd(H0,k0)\n",
    "            v00 = np.matmul(U0,W0)\n",
    "            V0 = v00\n",
    "                \n",
    "        if mode_value==1:\n",
    "            H1 = np.zeros([width, K], np.float32)\n",
    "            for i in range(0,number):\n",
    "                yangy11 = tl.tenalg.mode_dot(img[:,:,:,i],v00.T,mode=0)\n",
    "                yangy12 = tl.tenalg.mode_dot(yangy11,v1.T,mode=1)\n",
    "                yangy13 = tl.tenalg.mode_dot(yangy12,V2.T,mode=2)\n",
    "                yangunfold11 = tl.unfold(yangy13,mode=1)\n",
    "                U, S, V = np.linalg.svd((yangunfold11.T).dot(yangunfold11))\n",
    "                WQ1 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                uq11 = np.matmul(nx1[:,:,i],WQ1)\n",
    "                uq12 = np.matmul(uq11,WQ1.T)\n",
    "                uq13 = np.matmul(uq12,nx1[:,:,i].T)\n",
    "                uq1 = np.matmul(uq13,v1)\n",
    "                H1 = H1 + uq1\n",
    "            U1, Q1, W1 = decomposition.randomized_svd(H1,k1)\n",
    "            v11 = np.matmul(U1,W1)\n",
    "            v1 = v11\n",
    "            \n",
    "        if mode_value==2:\n",
    "            H2 = np.zeros([channel, k2], np.float32)\n",
    "            for i in range(0,number):\n",
    "                yangy21 = tl.tenalg.mode_dot(img[:,:,:,i],v00.T,mode=0)\n",
    "                yangy22 = tl.tenalg.mode_dot(yangy21,v11.T,mode=1)\n",
    "                yangy23 = tl.tenalg.mode_dot(yangy22,V2.T,mode=2)\n",
    "                yangunfold22 = tl.unfold(yangy23,mode=2)\n",
    "                U, S, V = np.linalg.svd(yangunfold22.dot(yangunfold22.T))\n",
    "                WQ2 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                uq21 = np.matmul(WQ2,nx2[:,:,i])\n",
    "                uq22 = np.matmul(uq21,nx2[:,:,i].T)\n",
    "                uq2 = np.matmul(uq22,WQ2.T)\n",
    "                H2 = H2 + uq2\n",
    "            U2, Q2, W2 = decomposition.randomized_svd(H2,k2)\n",
    "            v22 = np.matmul(U2,W2)\n",
    "            V2 = v22\n",
    "                \n",
    "    for i in range(0,number):\n",
    "        y = tl.tenalg.mode_dot(img[:,:,:,i],V0.T,mode=0)\n",
    "        yy = tl.tenalg.mode_dot(y,v1.T,mode=1)\n",
    "        yyy = tl.tenalg.mode_dot(yy,V2.T,mode=2)\n",
    "        yunfold = tl.unfold(yyy,mode=1)\n",
    "        Singel_Rec = np.linalg.norm(yunfold,ord='fro') ** 2\n",
    "        Obj_value += Singel_Rec\n",
    "    Obj_Rec[0,0] = Obj_value\n",
    "    print('Objective value:')\n",
    "    print('%.2f'%Obj_value)    \n",
    "     \n",
    "    for t in range(1,Tmax):\n",
    "        Obj_value = 0\n",
    "        \n",
    "        for mode_value in range(0,3):\n",
    "            if mode_value==0:\n",
    "                H0 = np.zeros([height, k0], np.float32)\n",
    "                for i in range(0,number):\n",
    "                    yangy01 = tl.tenalg.mode_dot(img[:,:,:,i],V0.T,mode=0)\n",
    "                    yangy02 = tl.tenalg.mode_dot(yangy01,v1.T,mode=1)\n",
    "                    yangy03 = tl.tenalg.mode_dot(yangy02,V2.T,mode=2)\n",
    "                    yangunfold00 = tl.unfold(yangy03,mode=0)\n",
    "                    U, S, V = np.linalg.svd(yangunfold00.dot(yangunfold00.T))\n",
    "                    WQ0 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                    uq01 = np.matmul(WQ0,nx0[:,:,i])\n",
    "                    uq02 = np.matmul(uq01,nx0[:,:,i].T)\n",
    "                    uq0 = np.matmul(uq02,WQ0.T)\n",
    "                    H0 = H0 + uq0\n",
    "                U0, Q0, W0 = decomposition.randomized_svd(H0,k0)\n",
    "                v00 = np.matmul(U0,W0)\n",
    "                \n",
    "            if mode_value==1:\n",
    "                H1 = np.zeros([width, K], np.float32)\n",
    "                for i in range(0,number):\n",
    "                    yangy11 = tl.tenalg.mode_dot(img[:,:,:,i],v00.T,mode=0)\n",
    "                    yangy12 = tl.tenalg.mode_dot(yangy11,v1.T,mode=1)\n",
    "                    yangy13 = tl.tenalg.mode_dot(yangy12,V2.T,mode=2)\n",
    "                    yangunfold11 = tl.unfold(yangy13,mode=1)\n",
    "                    U, S, V = np.linalg.svd((yangunfold11.T).dot(yangunfold11))\n",
    "                    WQ1 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                    uq11 = np.matmul(nx1[:,:,i],WQ1)\n",
    "                    uq12 = np.matmul(uq11,WQ1.T)\n",
    "                    uq13 = np.matmul(uq12,nx1[:,:,i].T)\n",
    "                    uq1 = np.matmul(uq13,v1)\n",
    "                    H1 = H1 + uq1\n",
    "                U1, Q1, W1 = decomposition.randomized_svd(H1,k1)\n",
    "                v11 = np.matmul(U1,W1)\n",
    "            \n",
    "            if mode_value==2:\n",
    "                H2 = np.zeros([channel, k2], np.float32)\n",
    "                for i in range(0,number):\n",
    "                    yangy21 = tl.tenalg.mode_dot(img[:,:,:,i],v00.T,mode=0)\n",
    "                    yangy22 = tl.tenalg.mode_dot(yangy21,v11.T,mode=1)\n",
    "                    yangy23 = tl.tenalg.mode_dot(yangy22,V2.T,mode=2)\n",
    "                    yangunfold22 = tl.unfold(yangy23,mode=2)\n",
    "                    U, S, V = np.linalg.svd(yangunfold22.dot(yangunfold22.T))\n",
    "                    WQ2 = U.dot(np.diag(S**(-1/4))).dot(V.T)\n",
    "                    uq21 = np.matmul(WQ2,nx2[:,:,i])\n",
    "                    uq22 = np.matmul(uq21,nx2[:,:,i].T)\n",
    "                    uq2 = np.matmul(uq22,WQ2.T)\n",
    "                    H2 = H2 + uq2\n",
    "                U2, Q2, W2 = decomposition.randomized_svd(H2,k2)\n",
    "                v22 = np.matmul(U2,W2)\n",
    "        \n",
    "        for i in range(0,number):\n",
    "            y = tl.tenalg.mode_dot(img[:,:,:,i],v00.T,mode=0)\n",
    "            yy = tl.tenalg.mode_dot(y,v11.T,mode=1)\n",
    "            yyy = tl.tenalg.mode_dot(yy,v22.T,mode=2)\n",
    "            yunfold = tl.unfold(yyy,mode=1)\n",
    "            Singel_Rec = np.linalg.norm(yunfold,ord='fro') ** 2\n",
    "            Obj_value += Singel_Rec\n",
    "        Obj_Rec[0,t] = Obj_value\n",
    "        print('%.2f'%Obj_value)\n",
    "        \n",
    "        #if Obj_Rec[0,t] < Obj_Rec[0,t-1]:\n",
    "        if (Obj_Rec[0,t]-Obj_Rec[0,t-1])/Obj_Rec[0,t-1] < 0.001:\n",
    "            np.savetxt(saveplace+\"0.txt\",v00)\n",
    "            np.savetxt(saveplace+\"1_%d.txt\"%K,v11)\n",
    "            np.savetxt(saveplace+\"2.txt\",v22)\n",
    "            break\n",
    "        else:\n",
    "            V0 = v00\n",
    "            v1 = v11\n",
    "            V2 = v22"
   ]
  },
